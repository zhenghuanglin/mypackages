<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>MySqlConnector</id>
    <version>0.44.1</version>
    <title>Async MySQL ADO.NET Connector</title>
    <authors>Bradley Grainger,Caleb Lloyd</authors>
    <owners>Bradley Grainger,Caleb Lloyd</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://mysql-net.github.io/MySqlConnector/</projectUrl>
    <iconUrl>https://mysql-net.github.io/MySqlConnector/img/logo.png</iconUrl>
    <description>A truly async MySQL ADO.NET provider, supporting MySQL Server, MariaDB, Percona Server, Amazon Aurora, Azure Database for MySQL and more.</description>
    <releaseNotes>https://mysql-net.github.io/MySqlConnector/overview/version-history/</releaseNotes>
    <copyright>Copyright 2016–2018 Bradley Grainger</copyright>
    <tags>mysql mysqlconnector async ado.net database netcore</tags>
    <repository type="git" url="https://github.com/mysql-net/MySqlConnector.git" />
    <dependencies>
